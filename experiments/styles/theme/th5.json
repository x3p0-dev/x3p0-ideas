						{
							"fontFamily": "Jost",
							"fontFeatureSettings": "'tnum', 'ss01' 1",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontVariant": "slashed-zero",
							"src": [
								"file:./public/fonts/jost/jost.woff2"
							]
						},
						{
							"fontFamily": "Jost",
							"fontFeatureSettings": "'tnum', 'ss01' 1",
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontVariant": "slashed-zero",
							"src": [
								"file:./public/fonts/jost/jost-italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "'Source Serif', Charter, 'Bitstream Charter', 'Sitka Text', Cambria, Georgia, serif",
					"slug": "secondary",
					"name": "Secondary (Source Serif)",
