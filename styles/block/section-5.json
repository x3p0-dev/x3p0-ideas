{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Style 5",
	"slug": "section-5",
	"blockTypes": [
		"core/calendar",
		"core/code",
		"core/column",
		"core/columns",
		"core/comment-content",
		"core/comment-template",
		"core/comments",
		"core/details",
		"core/file",
		"core/group",
		"core/media-text",
		"core/post-comments-form",
		"core/pullquote",
		"core/query",
		"core/quote",
		"x3p0/breadcrumbs"
	],
	"settings": {
		"custom": {
			"border": {
				"primary": {
					"colour": "var(--wp--preset--color--secondary-700)"
				}
			},
			"colour": {
				"background": {
					"level-0": "var(--wp--preset--color--secondary-800)",
					"level-1": "var(--wp--preset--color--secondary-900)",
					"level-2": "var(--wp--preset--color--secondary-950)",
					"accent": "var(--wp--preset--color--secondary-600)"
				},
				"foreground": {
					"primary": "var(--wp--preset--color--secondary-100)",
					"secondary": "var(--wp--preset--color--secondary-300)",
					"tertiary": "var(--wp--preset--color--secondary-500)",
					"accent": "var(--wp--preset--color--secondary-50)",
					"on-accent": "var(--wp--preset--color--secondary-50)"
				}
			},
			"elements": {
				"button": {
					"primary": {
						"border": {
							"colour": "transparent",
							"radius": "var(--wp--custom--border--primary--radius)",
							"style": "var(--wp--custom--border--primary--style)",
							"width": "var(--wp--custom--border--primary--width)"
						},
						"colour": {
							"background": "var(--wp--preset--color--secondary-600)",
							"foreground": "var(--wp--preset--color--secondary-50)"
						},
						"outline": {
							"colour": "transparent",
							"offset": "var(--wp--custom--outline--primary--offset)",
							"style": "var(--wp--custom--outline--primary--style)",
							"width": "var(--wp--custom--outline--primary--width)"
						},
						"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 60%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-600) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 60%, transparent))",
						"focus": {
							"outline": {
								"colour": "color-mix(in oklab, var(--wp--preset--color--secondary-600) 95%, var(--wp--preset--color--white))",
								"offset": "var(--wp--custom--elements--button--primary--outline--offset)",
								"style": "var(--wp--custom--elements--button--primary--outline--style)",
								"width": "var(--wp--custom--elements--button--primary--outline--width)"
							}
						},
						"hover": {
							"border": {
								"colour": "var(--wp--preset--color--secondary-900)",
								"radius": "var(--wp--custom--elements--button--primary--border--radius)",
								"style": "var(--wp--custom--elements--button--primary--border--style)",
								"width": "var(--wp--custom--elements--button--primary--border--width)"
							},
							"colour": {
								"background": "color-mix(in oklab, var(--wp--preset--color--secondary-600) 95%, var(--wp--preset--color--white))",
								"foreground": "var(--wp--preset--color--secondary-50)"
							},
							"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 100%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-600) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 100%, transparent))"
						}
					},
					"secondary": {
						"colour": {
							"background": "transparent",
							"foreground": "currentColor"
						},
						"border": {
							"colour": "currentColor",
							"radius": "var(--wp--custom--border--primary--radius)",
							"style": "var(--wp--custom--border--primary--style)",
							"width": "var(--wp--custom--border--primary--width)"
						},
						"outline": {
							"colour": "transparent",
							"offset": "var(--wp--custom--outline--primary--offset)",
							"style": "var(--wp--custom--outline--primary--style)",
							"width": "var(--wp--custom--outline--primary--width)"
						},
						"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 30%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 30%, transparent))",
						"focus": {
							"outline": {
								"colour": "currentColor",
								"offset": "var(--wp--custom--elements--button--secondary--outline--offset)",
								"style": "var(--wp--custom--elements--button--secondary--outline--style)",
								"width": "var(--wp--custom--elements--button--secondary--outline--width)"
							}
						},
						"hover": {
							"colour": {
								"background": "transparent",
								"foreground": "currentColor"
							},
							"border": {
								"colour": "light-dark(color-mix(in oklab, currentColor 90%, var(--wp--preset--color--black)), color-mix(in oklab, currentColor 50%, var(--wp--preset--color--white)))",
								"radius": "var(--wp--custom--elements--button--secondary--border--radius)",
								"style": "var(--wp--custom--elements--button--secondary--border--style)",
								"width": "var(--wp--custom--elements--button--secondary--border--width)"
							},
							"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 45%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 45%, transparent))"
						}
					}
				},
				"link": {
					"primary": {
						"colour": {
							"foreground": "var(--wp--preset--color--secondary-200)"
						},
						"hover": {
							"colour": {
								"foreground": "currentColor"
							}
						}
					},
					"secondary": {
						"colour": {
							"foreground": "currentColor"
						},
						"hover": {
							"colour": {
								"foreground": "var(--wp--preset--color--secondary-200)"
							}
						}
					}
				},
				"summary": {
					"colour": {
						"background": "transparent",
						"foreground": "currentColor"
					},
					"hover": {
						"colour": {
							"background": "var(--wp--custom--colour--background--level-2)",
							"foreground": "currentColor"
						}
					}
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--custom--colour--background--level-0)",
			"text": "var(--wp--custom--colour--foreground--primary)"
		}
	}
}
