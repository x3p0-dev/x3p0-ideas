{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Site Content",
	"slug": "site-content",
	"blockTypes": [
		"core/group",
		"core/template-part"
	],
	"styles": {
		"color": {
			"background": "var(--wp--custom--site-content--colour--background, var(--wp--custom--site-container--colour--background, var(--wp--custom--body--colour--background, var(--wp--custom--colour--background--level-0))))",
			"gradient": "var(--wp--custom--site-content--colour--gradient, none)",
			"text": "var(--wp--custom--site-content--colour--foreground, inherit)"
		}
	}
}
