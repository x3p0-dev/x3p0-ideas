{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 3,
	"title": "Style 2",
	"slug": "section-2",
	"blockTypes": [
		"core/calendar",
		"core/code",
		"core/column",
		"core/columns",
		"core/comment-content",
		"core/comment-template",
		"core/comments",
		"core/details",
		"core/group",
		"core/media-text",
		"core/post-comments-form",
		"core/preformatted",
		"core/pullquote",
		"core/query",
		"core/quote",
		"core/verse",
		"x3p0/breadcrumbs"
	],
	"settings": {
		"custom": {
			"border": {
				"primary": {
					"color": "light-dark(var(--wp--preset--color--neutral-300), var(--wp--preset--color--neutral-700))"
				},
				"button": {
					"primary": {
						"hover": {
							"color": "light-dark(var(--wp--preset--color--primary-800), var(--wp--preset--color--primary-900))"
						}
					}
				}
			},
			"color": {
				"surface": {
					"level-0": "light-dark(var(--wp--preset--color--neutral-100), var(--wp--preset--color--neutral-800))",
					"level-1": "light-dark(var(--wp--preset--color--neutral-200), var(--wp--preset--color--neutral-700))",
					"level-2": "light-dark(var(--wp--preset--color--neutral-300), var(--wp--preset--color--neutral-600))",
					"accent": "light-dark(var(--wp--preset--color--primary-600), var(--wp--preset--color--primary-400))",
					"button": {
						"primary": {
							"default": "light-dark(var(--wp--preset--color--primary-600), var(--wp--preset--color--primary-700))",
							"hover": "light-dark(var(--wp--preset--color--primary-800), var(--wp--preset--color--primary-600))"
						},
						"secondary": {
							"default": "transparent",
							"hover": "transparent"
						}
					}
				},
				"foreground": {
					"primary": "light-dark(var(--wp--preset--color--neutral-950), var(--wp--preset--color--neutral-100))",
					"secondary": "light-dark(var(--wp--preset--color--neutral-700), var(--wp--preset--color--neutral-300))",
					"tertiary": "light-dark(var(--wp--preset--color--neutral-400), var(--wp--preset--color--neutral-600))",
					"accent": "light-dark(var(--wp--preset--color--primary-700), var(--wp--preset--color--primary-300))",
					"on-accent": "light-dark(var(--wp--preset--color--white), var(--wp--preset--color--black))",
					"button": {
						"primary": {
							"default": "light-dark(var(--wp--preset--color--white), var(--wp--preset--color--white))",
							"hover": "light-dark(var(--wp--preset--color--primary-50), var(--wp--preset--color--primary-50))"
						},
						"secondary": {
							"default": "currentColor",
							"hover": "currentColor"
						}
					},
					"link": {
						"primary": {
							"default": "light-dark(var(--wp--preset--color--primary-700), var(--wp--preset--color--primary-300))",
							"hover": "currentColor"
						},
						"secondary": {
							"default": "currentColor",
							"hover": "light-dark(var(--wp--preset--color--primary-700), var(--wp--preset--color--primary-300))"
						}
					}
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--custom--color--surface--level-0)",
			"text": "var(--wp--custom--color--foreground--primary)"
		},
		"spacing": {
			"padding": {
				"top": "var(--wp--preset--spacing--70)",
				"bottom": "var(--wp--preset--spacing--70)",
				"left": "var(--wp--preset--spacing--70)",
				"right": "var(--wp--preset--spacing--70)"
			}
		}
	}
}
