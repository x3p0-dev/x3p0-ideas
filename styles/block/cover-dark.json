{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Dark",
	"slug": "cover-dark",
	"blockTypes": [
		"core/cover"
	],
	"settings": {
		"custom": {
			"border": {
				"primary": {
					"colour": "var(--wp--preset--color--neutral-700)"
				}
			},
			"colour": {
				"background": {
					"level-0": "var(--wp--preset--color--neutral-950)",
					"level-1": "var(--wp--preset--color--neutral-900)",
					"level-2": "var(--wp--preset--color--neutral-800)",
					"accent": "var(--wp--preset--color--primary-600)"
				},
				"foreground": {
					"primary": "var(--wp--preset--color--white)",
					"secondary": "var(--wp--preset--color--neutral-200)",
					"tertiary": "var(--wp--preset--color--neutral-400)",
					"accent": "var(--wp--preset--color--neutral-50)",
					"on-accent": "var(--wp--preset--color--white)"
				}
			},
			"elements": {
				"button": {
					"primary": {
						"border": {
							"colour": "transparent"
						},
						"colour": {
							"background": "var(--wp--preset--color--primary-600)",
							"foreground": "var(--wp--preset--color--white)"
						},
						"outline": {
							"colour": "transparent"
						},
						"shadow": "0 10px 15px -3px color-mix(in oklab, var(--wp--preset--color--primary-950) 80%, transparent), 0 4px 6px -4px color-mix(in oklab, var(--wp--preset--color--primary-950) 80%, transparent)",
						"focus": {
							"outline": {
								"colour": "var(--wp--preset--color--primary-700)"
							}
						},
						"hover": {
							"border": {
								"colour": "var(--wp--preset--color--primary-800)"
							},
							"colour": {
								"background": "var(--wp--preset--color--primary-700)",
								"foreground": "var(--wp--preset--color--primary-50)"
							},
							"shadow": "0 10px 15px -3px color-mix(in oklab, var(--wp--preset--color--primary-950) 100%, transparent), 0 4px 6px -4px color-mix(in oklab, var(--wp--preset--color--primary-950) 100%, transparent)"
						}
					},
					"secondary": {
						"colour": {
							"background": "transparent",
							"foreground": "currentColor"
						},
						"border": {
							"colour": "currentColor"
						},
						"outline": {
							"colour": "transparent"
						},
						"shadow": "0 10px 15px -3px color-mix(in oklab, var(--wp--preset--color--black) 30%, transparent), 0 4px 6px -4px color-mix(in oklab, var(--wp--preset--color--black) 30%, transparent)",
						"focus": {
							"outline": {
								"colour": "currentColor"
							}
						},
						"hover": {
							"colour": {
								"background": "transparent",
								"foreground": "currentColor"
							},
							"border": {
								"colour": "color-mix(in oklab, currentColor 50%, var(--wp--preset--color--white))"
							},
							"shadow": "0 10px 15px -3px color-mix(in oklab, var(--wp--preset--color--black) 45%, transparent), 0 4px 6px -4px color-mix(in oklab, var(--wp--preset--color--black) 45%, transparent)"
						}
					}
				},
				"link": {
					"primary": {
						"colour": {
							"foreground": "var(--wp--preset--color--neutral-200)"
						},
						"hover": {
							"colour": {
								"foreground": "currentColor"
							}
						}
					},
					"secondary": {
						"colour": {
							"foreground": "currentColor"
						},
						"hover": {
							"colour": {
								"foreground": "var(--wp--preset--color--white)"
							}
						}
					}
				},
				"summary": {
					"colour": {
						"background": "transparent",
						"foreground": "currentColor"
					},
					"hover": {
						"colour": {
							"background": "var(--wp--custom--colour--background--level-2)",
							"foreground": "currentColor"
						}
					}
				}
			}
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp--custom--colour--foreground--primary)"
		}
	}
}
