{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 3,
	"title": "Meta",
	"slug": "meta",
	"blockTypes": [ "core/group" ],
	"styles": {
		"color": {
			"text": "var:custom|color|text|muted"
		},
		"typography": {
			"fontSize": "var:preset|font-size|sm",
			"lineHeight": "var:custom|line-height|sm"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var:custom|color|text|link|secondary|default"
				},
				"typography": {
					"textDecoration": "none"
				},
				":focus": {
					"color": {
						"text": "var:custom|color|text|link|secondary|interact"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":hover": {
					"color": {
						"text": "var:custom|color|text|link|secondary|interact"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		}
	}
}
