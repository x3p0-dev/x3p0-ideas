{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Meta",
	"slug": "meta",
	"blockTypes": [ "core/group" ],
	"styles": {
		"color": {
			"text": "var(--wp--custom--colour--foreground--secondary)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--sm)",
			"lineHeight": "var(--wp--custom--line-height--sm)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--custom--elements--link--secondary--colour--foreground)"
				},
				"typography": {
					"textDecoration": "none"
				},
				":focus": {
					"color": {
						"text": "var(--wp--custom--elements--link--secondary--hover--colour--foreground)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--custom--elements--link--secondary--hover--colour--foreground)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		}
	}
}
