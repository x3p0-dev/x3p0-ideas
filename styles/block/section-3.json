{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 3,
	"title": "Style 3",
	"slug": "section-3",
	"blockTypes": [
		"core/calendar",
		"core/code",
		"core/column",
		"core/columns",
		"core/comment-content",
		"core/comment-template",
		"core/comments",
		"core/details",
		"core/group",
		"core/media-text",
		"core/post-comments-form",
		"core/preformatted",
		"core/pullquote",
		"core/query",
		"core/quote",
		"core/verse",
		"x3p0/breadcrumbs"
	],
	"settings": {
		"custom": {
			"border": {
				"primary": {
					"color": "var(--wp--preset--color--primary-100)"
				},
				"button": {
					"primary": {
						"hover": {
							"color": "var(--wp--preset--color--primary-900)"
						}
					}
				}
			},
			"color": {
				"surface": {
					"level-0": "var(--wp--preset--color--primary-800)",
					"level-1": "var(--wp--preset--color--primary-900)",
					"level-2": "var(--wp--preset--color--primary-950)",
					"accent": "var(--wp--preset--color--primary-600)",
					"button": {
						"primary": {
							"default": "var(--wp--preset--color--primary-600)",
							"hover": "color-mix(in oklab, var(--wp--preset--color--primary-600) 95%, var(--wp--preset--color--white))"
						},
						"secondary": {
							"default": "transparent",
							"hover": "transparent"
						}
					}
				},
				"foreground": {
					"primary": "var(--wp--preset--color--primary-100)",
					"secondary": "var(--wp--preset--color--primary-300)",
					"tertiary": "var(--wp--preset--color--primary-500)",
					"accent": "var(--wp--preset--color--primary-50)",
					"on-accent": "var(--wp--preset--color--primary-50)",
					"button": {
						"primary": {
							"default": "var(--wp--preset--color--primary-50)",
							"hover": "var(--wp--preset--color--primary-50)"
						},
						"secondary": {
							"default": "currentColor",
							"hover": "currentColor"
						}
					},
					"link": {
						"primary": {
							"default": "var(--wp--preset--color--primary-200)",
							"hover": "currentColor"
						},
						"secondary": {
							"default": "currentColor",
							"hover": "var(--wp--preset--color--primary-200)"
						}
					}
				}
			},
			"shadow": {
				"button": {
					"primary": {
						"default": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 60%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-600) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 60%, transparent))",
						"hover": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 100%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-600) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 100%, transparent))"
					}
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var:custom|color|surface|level-0",
			"text": "var:custom|color|foreground|primary"
		},
		"spacing": {
			"padding": {
				"top": "var:preset|spacing|70",
				"bottom": "var:preset|spacing|70",
				"left": "var:preset|spacing|70",
				"right": "var:preset|spacing|70"
			}
		}
	}
}
