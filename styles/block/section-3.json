{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Style 3",
	"slug": "section-3",
	"blockTypes": [
		"core/calendar",
		"core/code",
		"core/column",
		"core/columns",
		"core/comment-content",
		"core/comment-template",
		"core/comments",
		"core/details",
		"core/file",
		"core/group",
		"core/media-text",
		"core/post-comments-form",
		"core/pullquote",
		"core/query",
		"core/quote",
		"x3p0/breadcrumbs"
	],
	"settings": {
		"custom": {
			"color": {
				"background": {
					"level-0": "var(--wp--preset--color--primary-800)",
					"level-1": "var(--wp--preset--color--primary-900)",
					"level-2": "var(--wp--preset--color--primary-950)",
					"accent": "var(--wp--preset--color--primary-600)"
				},
				"foreground": {
					"primary": "var(--wp--preset--color--primary-100)",
					"secondary": "var(--wp--preset--color--primary-300)",
					"tertiary": "var(--wp--preset--color--primary-500)",
					"accent": "var(--wp--preset--color--primary-50)",
					"on-accent": "var(--wp--preset--color--primary-50)"
				},
				"border": {
					"accent-1": "var(--wp--preset--color--primary-50)",
					"bounds": "var(--wp--preset--color--primary-700)",
					"emphasis": "var(--wp--preset--color--primary-200)"
				}
			},
			"elements": {
				"button": {
					"primary": {
						"border": {
							"color": "transparent",
							"radius": "var(--wp--custom--border--global--radius)",
							"style": "var(--wp--custom--border--global--style)",
							"width": "var(--wp--custom--border--global--width)"
						},
						"color": {
							"background": "var(--wp--preset--color--primary-600)",
							"foreground": "var(--wp--preset--color--primary-50)"
						},
						"outline": {
							"color": "transparent",
							"offset": "var(--wp--custom--outline--primary--offset)",
							"style": "var(--wp--custom--outline--primary--style)",
							"width": "var(--wp--custom--outline--primary--width)"
						},
						"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 60%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-600) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 60%, transparent))",
						"focus": {
							"outline": {
								"color": "color-mix(in oklab, var(--wp--preset--color--primary-600) 95%, var(--wp--preset--color--white))",
								"offset": "var(--wp--custom--elements--button--primary--outline--offset)",
								"style": "var(--wp--custom--elements--button--primary--outline--style)",
								"width": "var(--wp--custom--elements--button--primary--outline--width)"
							}
						},
						"hover": {
							"border": {
								"color": "var(--wp--preset--color--primary-900)",
								"radius": "var(--wp--custom--elements--button--primary--border--radius)",
								"style": "var(--wp--custom--elements--button--primary--border--style)",
								"width": "var(--wp--custom--elements--button--primary--border--width)"
							},
							"color": {
								"background": "color-mix(in oklab, var(--wp--preset--color--primary-600) 95%, var(--wp--preset--color--white))",
								"foreground": "var(--wp--preset--color--primary-50)"
							},
							"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 100%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-600) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 100%, transparent))"
						}
					},
					"secondary": {
						"color": {
							"background": "transparent",
							"foreground": "currentColor"
						},
						"border": {
							"color": "currentColor",
							"radius": "var(--wp--custom--border--global--radius)",
							"style": "var(--wp--custom--border--global--style)",
							"width": "var(--wp--custom--border--global--width)"
						},
						"outline": {
							"color": "transparent",
							"offset": "var(--wp--custom--outline--primary--offset)",
							"style": "var(--wp--custom--outline--primary--style)",
							"width": "var(--wp--custom--outline--primary--width)"
						},
						"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 30%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 30%, transparent))",
						"focus": {
							"outline": {
								"color": "currentColor",
								"offset": "var(--wp--custom--elements--button--secondary--outline--offset)",
								"style": "var(--wp--custom--elements--button--secondary--outline--style)",
								"width": "var(--wp--custom--elements--button--secondary--outline--width)"
							}
						},
						"hover": {
							"color": {
								"background": "transparent",
								"foreground": "currentColor"
							},
							"border": {
								"color": "light-dark(color-mix(in oklab, currentColor 90%, var(--wp--preset--color--black)), color-mix(in oklab, currentColor 50%, var(--wp--preset--color--white)))",
								"radius": "var(--wp--custom--elements--button--secondary--border--radius)",
								"style": "var(--wp--custom--elements--button--secondary--border--style)",
								"width": "var(--wp--custom--elements--button--secondary--border--width)"
							},
							"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 45%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 45%, transparent))"
						}
					}
				},
				"link": {
					"primary": {
						"color": {
							"foreground": "var(--wp--preset--color--primary-200)"
						},
						"hover": {
							"color": {
								"foreground": "currentColor"
							}
						}
					},
					"secondary": {
						"color": {
							"foreground": "currentColor"
						},
						"hover": {
							"color": {
								"foreground": "var(--wp--preset--color--primary-200)"
							}
						}
					}
				},
				"summary": {
					"color": {
						"background": "transparent",
						"foreground": "currentColor"
					},
					"hover": {
						"color": {
							"background": "var(--wp--custom--color--background--level-2)",
							"foreground": "currentColor"
						}
					}
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--custom--color--background--level-0)",
			"text": "var(--wp--custom--color--foreground--primary)"
		}
	}
}
