{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Style 3",
	"slug": "section-3",
	"blockTypes": [
		"core/calendar",
		"core/code",
		"core/column",
		"core/columns",
		"core/comment-content",
		"core/comment-template",
		"core/comments",
		"core/details",
		"core/file",
		"core/group",
		"core/media-text",
		"core/post-comments-form",
		"core/pullquote",
		"core/query",
		"core/quote",
		"x3p0/breadcrumbs",
		"x3p0/media-data",
		"x3p0/media-data-field"
	],
	"settings": {
		"custom": {
			"color": {
				"background": {
					"level-0": "var(--wp--preset--color--primary-800)",
					"level-1": "var(--wp--preset--color--primary-900)",
					"level-2": "var(--wp--preset--color--primary-950)",
					"accent": "var(--wp--preset--color--primary-600)"
				},
				"foreground": {
					"default": "var(--wp--preset--color--primary-100)",
					"subtle": "var(--wp--preset--color--primary-300)",
					"muted": "var(--wp--preset--color--primary-500)",
					"accent": "var(--wp--preset--color--primary-50)",
					"on-accent": "var(--wp--preset--color--primary-50)"
				},
				"border": {
					"accent-1": "var(--wp--preset--color--primary-50)",
					"accent-2": "var(--wp--preset--color--secondary-50)",
					"bounds": "var(--wp--preset--color--primary-700)",
					"emphasis": "var(--wp--preset--color--primary-200)"
				},
				"button": {
					"elevated": {
						"background": "var(--wp--preset--color--primary-900)",
						"background-interact": "var(--wp--preset--color--primary-800)",
						"border": "var(--wp--preset--color--primary-700)",
						"border-interact": "var(--wp--preset--color--primary-600)",
						"foreground": "var(--wp--preset--color--primary-200)",
						"outline": "transparent",
						"outline-focus": "var(--wp--preset--color--primary-500)",
						"shadow": "var(--wp--preset--color--primary-900)",
						"shadow-interact": "var(--wp--preset--color--primary-900)"
					},
					"filled": {
						"background": "var(--wp--preset--color--primary-600)",
						"background-interact": "color-mix(in oklab, var(--wp--preset--color--primary-600) 95%, var(--wp--preset--color--white))",
						"border": "transparent",
						"border-interact": "var(--wp--preset--color--primary-700)",
						"foreground": "var(--wp--preset--color--primary-50)",
						"outline": "transparent",
						"outline-focus": "color-mix(in oklab, var(--wp--preset--color--primary-600) 95%, var(--wp--preset--color--white))",
						"shadow": "light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 60%, transparent))",
						"shadow-interact": "light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 100%, transparent))"
					},
					"tonal": {
						"background": "var(--wp--preset--color--primary-300)",
						"background-interact": "var(--wp--preset--color--primary-200)",
						"border": "transparent",
						"border-interact": "var(--wp--preset--color--primary-300)",
						"foreground": "var(--wp--preset--color--primary-900)",
						"outline": "transparent",
						"outline-focus": "var(--wp--preset--color--primary-200)",
						"shadow": "color-mix(in oklab, var(--wp--preset--color--primary-500) 40%, transparent)",
						"shadow-interact": "color-mix(in oklab, var(--wp--preset--color--primary-600) 40%, transparent)"
					}
				},
				"link": {
					"primary": {
						"foreground": "var(--wp--preset--color--primary-200)",
						"foreground-interact": "currentColor"
					},
					"secondary": {
						"foreground": "currentColor",
						"foreground-interact": "var(--wp--preset--color--primary-200)"
					}
				}
			},
			"shadow": {
				"global": {
					"default": "0 10px 15px -3px var(--wp--custom--color--shadow--default), 0 4px 6px -4px var(--wp--custom--color--shadow--default)",
					"interact": "0 10px 15px -3px var(--wp--custom--color--shadow--interact), 0 4px 6px -4px var(--wp--custom--color--shadow--interact)"
				},
				"button": {
					"elevated": {
						"default": "0 1px 3px 0 var(--wp--custom--color--button--elevated--shadow), 0 1px 2px -1px var(--wp--custom--color--button--elevated--shadow)",
						"interact": "0 1px 3px 0 var(--wp--custom--color--button--elevated--shadow-interact), 0 1px 2px -1px var(--wp--custom--color--button--elevated--shadow-interact)"
					},
					"filled": {
						"default": "none",
						"interact": "none"
					},
					"outlined": {
						"default": "none",
						"interact": "none"
					},
					"tonal": {
						"default": "none",
						"interact": "none"
					}
				}
			},
			"x3p0/progress": {
				"color": {
					"background": "var(--wp--preset--color--primary-200)",
					"foreground": "var(--wp--preset--color--primary-500)"
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--custom--color--background--level-0)",
			"text": "var(--wp--custom--color--foreground--default)"
		}
	}
}
