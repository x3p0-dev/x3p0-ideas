{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Style 6",
	"slug": "section-6",
	"blockTypes": [
		"core/calendar",
		"core/code",
		"core/column",
		"core/columns",
		"core/comment-content",
		"core/comment-template",
		"core/comments",
		"core/details",
		"core/file",
		"core/group",
		"core/media-text",
		"core/post-comments-form",
		"core/pullquote",
		"core/query",
		"core/quote",
		"x3p0/breadcrumbs",
		"x3p0/media-data",
		"x3p0/media-data-field"
	],
	"settings": {
		"custom": {
			"color": {
				"background": {
					"level-0": "light-dark(var(--wp--preset--color--secondary-100), var(--wp--preset--color--secondary-950))",
					"level-1": "light-dark(var(--wp--preset--color--secondary-200), var(--wp--preset--color--secondary-900))",
					"level-2": "light-dark(var(--wp--preset--color--secondary-300), var(--wp--preset--color--secondary-800))",
					"accent": "light-dark(var(--wp--preset--color--secondary-600), var(--wp--preset--color--secondary-700))"
				},
				"foreground": {
					"default": "light-dark(var(--wp--preset--color--secondary-950), var(--wp--preset--color--secondary-50))",
					"subtle": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary--200))",
					"muted": "light-dark(var(--wp--preset--color--secondary-400), var(--wp--preset--color--secondary-400))",
					"accent": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-100))",
					"on-accent": "light-dark(var(--wp--preset--color--secondary-50), var(--wp--preset--color--secondary-50))"
				},
				"border": {
					"accent-1": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-100))",
					"accent-2": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-100))",
					"bounds": "light-dark(var(--wp--preset--color--secondary-300), var(--wp--preset--color--secondary-700))",
					"emphasis": "light-dark(var(--wp--preset--color--secondary-800), var(--wp--preset--color--secondary-200))"
				},
				"shadow": {
					"default": "light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 30%, transparent))",
					"interact": "light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 45%, transparent))"
				},
				"button": {
					"elevated": {
						"background": "light-dark(var(--wp--preset--color--secondary-100), var(--wp--preset--color--secondary-900))",
						"background-interact": "light-dark(var(--wp--preset--color--secondary-200), var(--wp--preset--color--secondary-800))",
						"border": "light-dark(var(--wp--preset--color--secondary-500), var(--wp--preset--color--secondary-700))",
						"border-interact": "light-dark(var(--wp--preset--color--secondary-600), var(--wp--preset--color--secondary-600))",
						"foreground": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-200))",
						"outline": "transparent",
						"outline-focus": "light-dark(var(--wp--preset--color--secondary-500), var(--wp--preset--color--secondary-400))",
						"shadow": "light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 40%, transparent), var(--wp--preset--color--secondary-950))",
						"shadow-interact": "light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 40%, transparent), var(--wp--preset--color--secondary-950))"
					},
					"filled": {
						"background": "light-dark(var(--wp--preset--color--secondary-600), var(--wp--preset--color--secondary-700))",
						"background-interact": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-600))",
						"border": "transparent",
						"border-interact": "light-dark(var(--wp--preset--color--secondary-800), var(--wp--preset--color--secondary-900))",
						"foreground": "var(--wp--preset--color--white)",
						"outline": "transparent",
						"outline-focus": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-600))",
						"shadow": "light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-600) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 60%, transparent))",
						"shadow-interact": "light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-600) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 100%, transparent))"
					},
					"tonal": {
						"background": "light-dark(var(--wp--preset--color--secondary-300), var(--wp--preset--color--secondary-300))",
						"background-interact": "light-dark(var(--wp--preset--color--secondary-300), var(--wp--preset--color--secondary-300))",
						"border": "transparent",
						"border-interact": "light-dark(var(--wp--preset--color--secondary-400), var(--wp--preset--color--secondary-400))",
						"foreground": "light-dark(var(--wp--preset--color--secondary-950), var(--wp--preset--color--secondary-950))",
						"outline": "transparent",
						"outline-focus": "light-dark(var(--wp--preset--color--secondary-300), var(--wp--preset--color--secondary-300))",
						"shadow": "light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-500) 80%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 60%, transparent))",
						"shadow-interact": "light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-300) 75%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 60%, transparent))"
					}
				},
				"link": {
					"primary": {
						"foreground": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-300))",
						"foreground-interact": "currentColor"
					},
					"secondary": {
						"foreground": "currentColor",
						"foreground-interact": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-300))"
					}
				}
			},
			"shadow": {
				"global": {
					"default": "0 10px 15px -3px var(--wp--custom--color--shadow--default), 0 4px 6px -4px var(--wp--custom--color--shadow--default)",
					"interact": "0 10px 15px -3px var(--wp--custom--color--shadow--interact), 0 4px 6px -4px var(--wp--custom--color--shadow--interact)"
				},
				"button": {
					"elevated": {
						"default": "0 1px 3px 0 var(--wp--custom--color--button--elevated--shadow), 0 1px 2px -1px var(--wp--custom--color--button--elevated--shadow)",
						"interact": "0 1px 3px 0 var(--wp--custom--color--button--elevated--shadow-interact), 0 1px 2px -1px var(--wp--custom--color--button--elevated--shadow-interact)"
					},
					"filled": {
						"default": "none",
						"interact": "none"
					},
					"outlined": {
						"default": "none",
						"interact": "none"
					},
					"tonal": {
						"default": "none",
						"interact": "none"
					}
				}
			},
			"x3p0/progress": {
				"color": {
					"background": "var(--wp--preset--color--secondary-300)",
					"foreground": "var(--wp--preset--color--secondary-600)"
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--custom--color--background--level-0)",
			"text": "var(--wp--custom--color--foreground--default)"
		}
	}
}
