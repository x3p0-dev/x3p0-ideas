{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Style 6",
	"slug": "section-6",
	"blockTypes": [
		"core/calendar",
		"core/code",
		"core/column",
		"core/columns",
		"core/comment-content",
		"core/comment-template",
		"core/comments",
		"core/details",
		"core/file",
		"core/group",
		"core/media-text",
		"core/post-comments-form",
		"core/pullquote",
		"core/query",
		"core/quote",
		"x3p0/breadcrumbs"
	],
	"settings": {
		"custom": {
			"color": {
				"background": {
					"level-0": "light-dark(var(--wp--preset--color--secondary-100), var(--wp--preset--color--secondary-950))",
					"level-1": "light-dark(var(--wp--preset--color--secondary-200), var(--wp--preset--color--secondary-900))",
					"level-2": "light-dark(var(--wp--preset--color--secondary-300), var(--wp--preset--color--secondary-800))",
					"accent": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-400))"
				},
				"foreground": {
					"primary": "light-dark(var(--wp--preset--color--secondary-950), var(--wp--preset--color--secondary-50))",
					"secondary": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary--200))",
					"tertiary": "light-dark(var(--wp--preset--color--secondary-400), var(--wp--preset--color--secondary-400))",
					"accent": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-100))",
					"on-accent": "light-dark(var(--wp--preset--color--secondary-50), var(--wp--preset--color--secondary-950))"
				},
				"border": {
					"accent-1": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-100))",
					"bounds": "light-dark(var(--wp--preset--color--secondary-300), var(--wp--preset--color--secondary-700))",
					"emphasis": "light-dark(var(--wp--preset--color--secondary-800), var(--wp--preset--color--secondary-200))"
				}
			},
			"elements": {
				"button": {
					"primary": {
						"border": {
							"color": "transparent",
							"radius": "var(--wp--custom--border--global--radius)",
							"style": "var(--wp--custom--border--global--style)",
							"width": "var(--wp--custom--border--global--width)"
						},
						"color": {
							"background": "light-dark(var(--wp--preset--color--secondary-600), var(--wp--preset--color--secondary-700))",
							"foreground": "light-dark(var(--wp--preset--color--white), var(--wp--preset--color--white))"
						},
						"outline": {
							"color": "transparent",
							"offset": "var(--wp--custom--outline--primary--offset)",
							"style": "var(--wp--custom--outline--primary--style)",
							"width": "var(--wp--custom--outline--primary--width)"
						},
						"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-600) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 60%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-600) 40%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 60%, transparent))",
						"focus": {
							"outline": {
								"color": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-600))",
								"offset": "var(--wp--custom--elements--button--primary--outline--offset)",
								"style": "var(--wp--custom--elements--button--primary--outline--style)",
								"width": "var(--wp--custom--elements--button--primary--outline--width)"
							}
						},
						"hover": {
							"border": {
								"color": "light-dark(var(--wp--preset--color--secondary-800), var(--wp--preset--color--secondary-900))",
								"radius": "var(--wp--custom--elements--button--primary--border--radius)",
								"style": "var(--wp--custom--elements--button--primary--border--style)",
								"width": "var(--wp--custom--elements--button--primary--border--width)"
							},
							"color": {
								"background": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-600))",
								"foreground": "light-dark(var(--wp--preset--color--secondary-50), var(--wp--preset--color--secondary-50))"
							},
							"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-600) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 100%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-600) 60%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 100%, transparent))"
						}
					},
					"secondary": {
						"color": {
							"background": "transparent",
							"foreground": "currentColor"
						},
						"border": {
							"color": "currentColor",
							"radius": "var(--wp--custom--border--global--radius)",
							"style": "var(--wp--custom--border--global--style)",
							"width": "var(--wp--custom--border--global--width)"
						},
						"outline": {
							"color": "transparent",
							"offset": "var(--wp--custom--outline--primary--offset)",
							"style": "var(--wp--custom--outline--primary--style)",
							"width": "var(--wp--custom--outline--primary--width)"
						},
						"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 30%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 30%, transparent))",
						"focus": {
							"outline": {
								"color": "currentColor",
								"offset": "var(--wp--custom--elements--button--secondary--outline--offset)",
								"style": "var(--wp--custom--elements--button--secondary--outline--style)",
								"width": "var(--wp--custom--elements--button--secondary--outline--width)"
							}
						},
						"hover": {
							"color": {
								"background": "transparent",
								"foreground": "currentColor"
							},
							"border": {
								"color": "light-dark(color-mix(in oklab, currentColor 90%, var(--wp--preset--color--black)), color-mix(in oklab, currentColor 50%, var(--wp--preset--color--white)))",
								"radius": "var(--wp--custom--elements--button--secondary--border--radius)",
								"style": "var(--wp--custom--elements--button--secondary--border--style)",
								"width": "var(--wp--custom--elements--button--secondary--border--width)"
							},
							"shadow": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 45%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--black) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--black) 45%, transparent))"
						}
					}
				},
				"link": {
					"primary": {
						"color": {
							"foreground": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-300))"
						},
						"hover": {
							"color": {
								"foreground": "currentColor"
							}
						}
					},
					"secondary": {
						"color": {
							"foreground": "currentColor"
						},
						"hover": {
							"color": {
								"foreground": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-300))"
							}
						}
					}
				},
				"summary": {
					"color": {
						"background": "transparent",
						"foreground": "currentColor"
					},
					"hover": {
						"color": {
							"background": "var(--wp--custom--color--background--level-2)",
							"foreground": "currentColor"
						}
					}
				}
			},
			"shadow": {
				"primary": {
					"default": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 30%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 30%, transparent))",
					"hover": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 45%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--secondary-950) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--secondary-950) 45%, transparent))"
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--custom--color--background--level-0)",
			"text": "var(--wp--custom--color--foreground--primary)"
		}
	}
}
