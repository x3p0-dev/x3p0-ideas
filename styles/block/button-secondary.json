{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Secondary",
	"slug": "button-secondary",
	"blockTypes": [ "core/button" ],
	"styles": {
		"border": {
			"color": "var(--wp--custom--elements--button--secondary--border--colour)",
			"radius": "var(--wp--custom--elements--button--secondary--border--radius)",
			"style": "var(--wp--custom--elements--button--secondary--border--style)",
			"width": "var(--wp--custom--elements--button--secondary--border--width)"
		},
		"color": {
			"background": "var(--wp--custom--elements--button--secondary--colour--background)",
			"text": "var(--wp--custom--elements--button--secondary--colour--foreground)"
		},
		"outline": {
			"color": "var(--wp--custom--elements--button--secondary--outline--colour)",
			"offset": "var(--wp--custom--elements--button--secondary--outline--offset)",
			"style": "var(--wp--custom--elements--button--secondary--outline--style)",
			"width": "var(--wp--custom--elements--button--secondary--outline--width)"
		},
		"shadow": "var(--wp--custom--elements--button--secondary--shadow)",
		"spacing": {
			"padding": {
				"top": {
					"ref": "styles.elements.button.spacing.padding.top"
				},
				"bottom": {
					"ref": "styles.elements.button.spacing.padding.bottom"
				},
				"left": {
					"ref": "styles.elements.button.spacing.padding.left"
				},
				"right": {
					"ref": "styles.elements.button.spacing.padding.right"
				}
			}
		}
	}
}
