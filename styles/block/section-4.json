{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 3,
	"title": "Style 4",
	"slug": "section-4",
	"blockTypes": [
		"core/calendar",
		"core/code",
		"core/column",
		"core/columns",
		"core/comment-content",
		"core/comment-template",
		"core/comments",
		"core/details",
		"core/group",
		"core/media-text",
		"core/post-comments-form",
		"core/preformatted",
		"core/pullquote",
		"core/query",
		"core/quote",
		"core/verse",
		"x3p0/breadcrumbs"
	],
	"settings": {
		"custom": {
			"color": {
				"surface": {
					"level-0": "light-dark(var(--wp--preset--color--secondary-700), var(--wp--preset--color--secondary-800))",
					"level-1": "light-dark(var(--wp--preset--color--secondary-800), var(--wp--preset--color--secondary-900))",
					"level-2": "light-dark(var(--wp--preset--color--secondary-900), var(--wp--preset--color--secondary-950))",
					"accent": "light-dark(var(--wp--preset--color--secondary-300), var(--wp--preset--color--secondary-300))",
					"interactive": {
						"primary": {
							"default": "light-dark(var(--wp--preset--color--primary-900), var(--wp--preset--color--primary-600))",
							"hover": "light-dark(var(--wp--preset--color--primary-950), var(--wp--preset--color--primary-700))"
						},
						"secondary": {
							"default": "transparent",
							"hover": "transparent"
						}
					}
				},
				"foreground": {
					"primary": "light-dark(var(--wp--preset--color--secondary-50), var(--wp--preset--color--secondary-100))",
					"secondary": "light-dark(var(--wp--preset--color--secondary-200), var(--wp--preset--color--secondary-300))",
					"tertiary": "light-dark(var(--wp--preset--color--secondary-500), var(--wp--preset--color--secondary-500))",
					"accent": "light-dark(var(--wp--preset--color--secondary-300), var(--wp--preset--color--secondary-300))",
					"on-accent": "light-dark(var(--wp--preset--color--secondary-800), var(--wp--preset--color--secondary-800))",
					"interactive": {
						"primary": {
							"default": "light-dark(var(--wp--preset--color--white), var(--wp--preset--color--white))",
							"hover": "light-dark(var(--wp--preset--color--primary-50), var(--wp--preset--color--primary-50))"
						},
						"secondary": {
							"default": "currentColor",
							"hover": "currentColor"
						}
					},
					"link": {
						"primary": {
							"default": "light-dark(var(--wp--preset--color--white), var(--wp--preset--color--secondary-200))",
							"hover": "currentColor"
						},
						"secondary": {
							"default": "currentColor",
							"hover": "light-dark(var(--wp--preset--color--white), var(--wp--preset--color--secondary-200))"
						}
					}
				},
				"border": {
					"primary": "light-dark(var(--wp--preset--color--secondary-900), var(--wp--preset--color--secondary-100))",
					"secondary": "light-dark(var(--wp--preset--color--secondary-800), var(--wp--preset--color--secondary-200))"
				}
			}
		}
	},
	"styles": {
		"border": {
			"color": "var:custom|color|surface|level-1"
		},
		"color": {
			"background": "var:custom|color|surface|level-0",
			"text": "var:custom|color|foreground|primary"
		},
		"spacing": {
			"padding": {
				"top": "var:preset|spacing|70",
				"bottom": "var:preset|spacing|70",
				"left": "var:preset|spacing|70",
				"right": "var:preset|spacing|70"
			}
		}
	}
}
