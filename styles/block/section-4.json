{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 3,
	"title": "Style 4",
	"slug": "section-4",
	"blockTypes": [
		"core/calendar",
		"core/code",
		"core/column",
		"core/columns",
		"core/comment-content",
		"core/comment-template",
		"core/comments",
		"core/details",
		"core/group",
		"core/media-text",
		"core/post-comments-form",
		"core/preformatted",
		"core/pullquote",
		"core/query",
		"core/quote",
		"core/verse",
		"x3p0/breadcrumbs"
	],
	"settings": {
		"custom": {
			"border": {
				"primary": {
					"color": "light-dark(var(--wp--preset--color--primary-300), var(--wp--preset--color--primary-700))"
				}
			},
			"color": {
				"surface": {
					"level-0": "light-dark(var(--wp--preset--color--primary-100), var(--wp--preset--color--primary-950))",
					"level-1": "light-dark(var(--wp--preset--color--primary-200), var(--wp--preset--color--primary-900))",
					"level-2": "light-dark(var(--wp--preset--color--primary-300), var(--wp--preset--color--primary-800))",
					"accent": "light-dark(var(--wp--preset--color--primary-700), var(--wp--preset--color--primary-400))"
				},
				"foreground": {
					"primary": "light-dark(var(--wp--preset--color--primary-950), var(--wp--preset--color--primary-50))",
					"secondary": "light-dark(var(--wp--preset--color--primary-700), var(--wp--preset--color--primary--200))",
					"tertiary": "light-dark(var(--wp--preset--color--primary-400), var(--wp--preset--color--primary-400))",
					"accent": "light-dark(var(--wp--preset--color--primary-700), var(--wp--preset--color--primary-100))",
					"on-accent": "light-dark(var(--wp--preset--color--primary-50), var(--wp--preset--color--primary-950))"
				}
			},
			"shadow": {
				"primary": {
					"default": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 30%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 10%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 30%, transparent))",
					"hover": "0 10px 15px -3px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 45%, transparent)), 0 4px 6px -4px light-dark(color-mix(in oklab, var(--wp--preset--color--primary-950) 25%, transparent), color-mix(in oklab, var(--wp--preset--color--primary-950) 45%, transparent))"
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--custom--color--surface--level-0)",
			"text": "var(--wp--custom--color--foreground--primary)"
		},
		"spacing": {
			"padding": {
				"top": "var(--wp--preset--spacing--70)",
				"bottom": "var(--wp--preset--spacing--70)",
				"left": "var(--wp--preset--spacing--70)",
				"right": "var(--wp--preset--spacing--70)"
			}
		}
	}
}
