{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Geo Source",
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Jost, ui-sans-serif, sans-serif",
					"slug": "primary",
					"name": "Primary (Jost)",
					"fontFace": [
						{
							"fontFamily": "Jost",
							"fontFeatureSettings": "'tnum', 'ss01' 1",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontVariant": "slashed-zero",
							"src": [
								"file:./public/fonts/jost/jost.woff2"
							]
						},
						{
							"fontFamily": "Jost",
							"fontFeatureSettings": "'tnum', 'ss01' 1",
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontVariant": "slashed-zero",
							"src": [
								"file:./public/fonts/jost/jost-italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "'Source Serif', Charter, 'Bitstream Charter', 'Sitka Text', Cambria, Georgia, serif",
					"slug": "secondary",
					"name": "Secondary (Source Serif)",
					"fontFace": [
						{
							"fontFamily": "Source Serif",
							"fontWeight": "200 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./public/fonts/source/source-serif.woff2"
							]
						},
						{
							"fontFamily": "Source Serif",
							"fontWeight": "200 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [
								"file:./public/fonts/source/source-serif-italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "ui-monospace, 'Fira Code', 'Cascadia Code', 'Source Code Pro', Menlo, Consolas, 'DejaVu Sans Mono', monospace",
					"slug": "mono",
					"name": "Monospace"
				}
			]
		}
	},
	"styles": {
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": "500"
				}
			},
			"caption": {
				"typography": {
					"fontStyle": "italic"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "800"
				}
			}
		},
		"blocks": {
			"core/file": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": "500"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
			"core/post-comments-form": {
				"elements": {
					"h3": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--lg)",
							"lineHeight": "var(--wp--custom--line-height--lg)"
						}
					}
				}
			},
			"core/post-comments-count": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
			"core/post-comments-link": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
			"core/post-author-biography": {
				"typography": {
					"fontStyle": "italic",
					"lineHeight": "var(--wp--custom--font-size--lg)"
				}
			},
			"core/pullquote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--tertiary)",
					"fontStyle": "italic"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--primary)",
							"fontStyle": "normal"
						}
					}
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--sm)",
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--line-height--sm)"
				}
			},
			"core/quote": {
				"typography": {
					"fontStyle": "italic"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--primary)",
							"fontStyle": "normal"
						}
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--preset--font-size--md)",
					"fontStyle": "italic",
					"fontWeight": "400",
					"letterSpacing": "0",
					"textTransform": "none"
				}
			},
			"core/social-links": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			},
			"core/verse": {
				"typography": {
					"fontStyle": "italic"
				}
			},
			"x3p0/breadcrumbs": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": "400",
					"fontSize": "var(--wp--preset--font-size--sm)",
					"lineHeight": "var(--wp--custom--line-height--sm)"
				}
			}
		},
		"variations": {
			"meta": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)"
				}
			}
		}
	}
}
