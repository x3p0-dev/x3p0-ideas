{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/wp/6.8/schemas/json/theme.json",
	"version": 3,
	"title": "Notes",
	"settings": {
		"custom": {
			"variation": "notes"
		}
	},
	"styles": {
		"css": "@layer variation { body { background: var(--wp--preset--gradient--emerge); } }",
		"color": {
			"gradient": "var(--wp--preset--gradient--emerge)"
		},
		"variations": {
			"site-container": {
				"css": "contain: paint; max-width: 56rem; width: 100%; & > * { margin-top: 0; }",
				"border": {
					"radius": "clamp(0rem, calc(0.23583 * (100cqi - 56rem)), var(--wp--custom--border--global--radius))"
				},
				"shadow": "var(--wp--custom--shadow--primary--default)",
				"spacing": {
					"margin": {
						"top": "clamp(0rem, calc(0.23583 * (100cqi - 56rem)), 5.66rem) !important",
						"bottom": "clamp(0rem, calc(0.23583 * (100cqi - 56rem)), 5.66rem) !important",
						"left": "auto",
						"right": "auto"
					}
				}
			}
		}
	}
}
