{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Boundaries",
	"settings": {
		"blocks": {
			"core/gallery": {
				"custom": {
					"caption": {
						"background": "rgb(0 0 0 / 50%)",
						"padding": "var:preset|spacing|40"
					}
				}
			}
		},
		"custom": {
			"variation": "boundaries",
			"color": {
				"text": {
					"link": {
						"primary": {
							"default": "currentColor",
							"hover": "var:preset|color|primary-700"
						}
					}
				},
				"border": {
					"default": "var:preset|color|neutral-300"
				}
			},
			"section-1": {
				"color": {
					"text": {
						"link": {
							"primary": {
								"default": "currentColor",
								"hover": "var:preset|color|primary-700"
							}
						}
					},
					"border": {
						"default": "var:preset|color|neutral-300"
					}
				}
			},
			"section-2": {
				"color": {
					"text": {
						"link": {
							"primary": {
								"default": "currentColor",
								"hover": "var:preset|color|primary-700"
							}
						}
					},
					"border": {
						"default": "var:preset|color|neutral-400"
					}
				}
			},
			"global": {
				"border": {
					"style": "double",
					"width": "4px",
					"radius": "0"
				}
			},
			"pattern": {
				"footerCallToAction": {
					"background": "transparent"
				},
				"footerLinks": {
					"background": "transparent"
				}
			},
			"siteBlocks": {
				"borderColor": "var:custom|global|border|color",
				"borderStyle": "var:custom|global|border|style",
				"borderWidth": "var:custom|global|border|width",
				"margin": "var:preset|spacing|70"
			}
		},
		"color": {
			"palette": [
				{
					"slug": "primary-950",
					"color": "#020617",
					"name": "Primary: 950"
				},
				{
					"slug": "primary-900",
					"color": "#831843",
					"name": "Primary: 900"
				},
				{
					"slug": "primary-800",
					"color": "#9d174f",
					"name": "Primary: 800"
				},
				{
					"slug": "primary-700",
					"color": "#be185d",
					"name": "Primary: 700"
				},
				{
					"slug": "primary-600",
					"color": "#db2772",
					"name": "Primary: 600"
				},
				{
					"slug": "primary-500",
					"color": "#ec4899",
					"name": "Primary: 500"
				},
				{
					"slug": "primary-400",
					"color": "#f472a8",
					"name": "Primary: 400"
				},
				{
					"slug": "primary-300",
					"color": "#f9a8d4",
					"name": "Primary: 300"
				},
				{
					"slug": "primary-200",
					"color": "#fbcfe1",
					"name": "Primary: 200"
				},
				{
					"slug": "primary-100",
					"color": "#fce7f3",
					"name": "Primary: 100"
				},
				{
					"slug": "primary-50",
					"color": "#fdf2f8",
					"name": "Primary: 50"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "neutral-950",
					"color": "#030712",
					"name": "Neutral: 950"
				},
				{
					"slug": "neutral-900",
					"color": "#111827",
					"name": "Neutral: 900"
				},
				{
					"slug": "neutral-800",
					"color": "#1f2937",
					"name": "Neutral: 800"
				},
				{
					"slug": "neutral-700",
					"color": "#374151",
					"name": "Neutral: 700"
				},
				{
					"slug": "neutral-600",
					"color": "#4b5563",
					"name": "Neutral: 600"
				},
				{
					"slug": "neutral-500",
					"color": "#6b7280",
					"name": "Neutral: 500"
				},
				{
					"slug": "neutral-400",
					"color": "#9ca3af",
					"name": "Neutral: 400"
				},
				{
					"slug": "neutral-300",
					"color": "#d1d5db",
					"name": "Neutral: 300"
				},
				{
					"slug": "neutral-200",
					"color": "#e5e7eb",
					"name": "Neutral: 200"
				},
				{
					"slug": "neutral-100",
					"color": "#f3f4f6",
					"name": "Neutral: 100"
				},
				{
					"slug": "neutral-50",
					"color": "#f9fafb",
					"name": "Neutral: 50"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Jost, ui-sans-serif, sans-serif",
					"slug": "primary",
					"name": "Primary (Jost)",
					"fontFace": [
						{
							"fontFamily": "Jost",
							"fontFeatureSettings": "'tnum', 'ss01' 1",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontVariant": "slashed-zero",
							"src": [
								"file:./public/fonts/jost/jost.woff2"
							]
						},
						{
							"fontFamily": "Jost",
							"fontFeatureSettings": "'tnum', 'ss01' 1",
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontVariant": "slashed-zero",
							"src": [
								"file:./public/fonts/jost/jost-italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "Cabin, sans-serif",
					"slug": "secondary",
					"name": "Secondary (Cabin)",
					"fontFace": [
						{
							"fontFamily": "Cabin",
							"fontWeight": "400 700",
							"fontStyle": "normal",
							"fontStretch": "75% 100%",
							"src": [
								"file:./public/fonts/cabin/cabin.woff2"
							]
						},
						{
							"fontFamily": "Cabin",
							"fontWeight": "400 700",
							"fontStyle": "italic",
							"fontStretch": "75% 100%",
							"src": [
								"file:./public/fonts/cabin/cabin-italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "'JetBrains Mono', ui-monospace, 'Fira Code', 'Cascadia Code', 'Source Code Pro', Menlo, Consolas, 'DejaVu Sans Mono', monospace",
					"slug": "mono",
					"name": "Monospace"
				}
			]
		}
	},
	"styles": {
		"elements": {
			"heading": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|2-xl",
					"lineHeight": "var:custom|line-height|2-xl"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|xl",
					"lineHeight": "var:custom|line-height|xl"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|lg",
					"lineHeight": "var:custom|line-height|lg"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|md",
					"lineHeight": "var:custom|line-height|md"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|sm",
					"lineHeight": "var:custom|line-height|sm"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|xs",
					"lineHeight": "var:custom|line-height|xs"
				}
			}
		},
		"blocks": {
			"core/calendar": {
				"color": {
					"background": "transparent"
				}
			},
			"core/code": {
				"color": {
					"background": "transparent"
				}
			},
			"core/details": {
				"color": {
					"background": "transparent"
				}
			},
			"core/file": {
				"color": {
					"background": "transparent"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":focus": {
							"color": {
								"text": "var:preset|color|primary-700"
							}
						},
						":hover": {
							"color": {
								"text": "var:preset|color|primary-700"
							}
						}
					}
				}
			},
			"core/post-comments-form": {
				"elements": {
					"h3": {
						"typography": {
							"fontSize": "var:preset|font-size|lg",
							"lineHeight": "var:custom|line-height|lg"
						}
					}
				}
			},
			"core/post-featured-image": {
				"border": {
					"color": "var:custom|global|border|color",
					"style": "var:custom|global|border|style",
					"width": "var:custom|global|border|width",
					"radius": "var:custom|global|border|radius"
				},
				"shadow": "var:custom|global|shadow"
			},
			"core/pullquote": {
				"typography": {
					"fontStyle": "italic"
				},
				"elements": {
					"cite": {
						"color": {
							"text": "var:custom|color|text|muted"
						},
						"typography": {
							"fontStyle": "normal"
						}
					}
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var:preset|font-size|md",
					"fontWeight": "500",
					"lineHeight": "var:custom|line-height|md"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/quote": {
				"typography": {
					"fontStyle": "italic"
				},
				"elements": {
					"cite": {
						"color": {
							"text": "var:custom|color|text|muted"
						},
						"typography": {
							"fontStyle": "normal"
						}
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/verse": {
				"typography": {
					"fontStyle": "italic"
				}
			}
		},
		"variations": {
			"archive-header": {
				"border": {
					"bottom": {
						"color": "var:custom|global|border|color",
						"style": "var:custom|global|border|style",
						"width": "var:custom|global|border|width"
					}
				}
			},
			"meta": {
				"color": {
					"text": "var:custom|color|text|muted"
				}
			},
			"site-footer": {
				"color": {
					"background": "transparent"
				},
				"border": {
					"top": {
						"color": "var:custom|global|border|color",
						"style": "var:custom|global|border|style",
						"width": "var:custom|global|border|width"
					}
				}
			},
			"site-header": {
				"border": {
					"bottom": {
						"color": "var:custom|global|border|color",
						"style": "var:custom|global|border|style",
						"width": "var:custom|global|border|width"
					}
				},
				"blocks": {
					"x3p0/breadcrumbs": {
						"border": {
							"top": {
								"color": "var:custom|global|border|color",
								"style": "var:custom|global|border|style",
								"width": "var:custom|global|border|width"
							}
						},
						"color": {
							"background": "transparent"
						},
						"typography": {
							"fontSize": "var:preset|font-size|sm",
							"lineHeight": "var:custom|line-height|sm"
						}
					}
				}
			}
		}
	}
}
