{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Memories",
	"settings": {
		"blocks": {
			"core/details": {
				"custom": {
					"elements": {
						"summary": {
							"fontFamily": "var(--wp--preset|font-family|secondary)"
						}
					}
				}
			},
			"core/gallery": {
				"custom": {
					"caption": {
						"background": "rgb(0 0 0 / 30%)",
						"padding": "var(--wp--preset|spacing|40)"
					}
				}
			},
			"core/navigation-submenu": {
				"custom": {
					"block": {
						"navigation-submenu": {
							"min-width": "18rem"
						}
					}
				}
			},
			"core/table": {
				"custom": {
					"stripes": {
						"stripeBackground": "var(--wp--preset|color|neutral-100)"
					}
				}
			}
		},
		"custom": {
			"variation": "memories",
			"color": {
				"background": {
					"submenu": {
						"default": "var(--wp--preset|color|primary-700)",
						"hover": "var(--wp--preset|color|primary-900)"
					}
				},
				"text": {
					"submenu": {
						"default": "var(--wp--preset|color|primary-50)",
						"hover": "var(--wp--preset|color|primary-50)"
					}
				}
			},
			"section-1": {
				"color": {
					"border": {
						"default": "var(--wp--preset|color|neutral-200)"
					}
				}
			},
			"section-2": {
				"color": {
					"border": {
						"default": "var(--wp--preset|color|neutral-200)"
					}
				}
			},
			"section-3": {
				"color": {
					"background": {
						"level-0": "var(--wp--preset|color|primary-700)",
						"level-1": "var(--wp--preset|color|primary-800)",
						"level-2": "var(--wp--preset|color|primary-900)",
						"button": {
							"primary": {
								"default": "var(--wp--preset|color|primary-900)",
								"hover": "var(--wp--preset|color|primary-950)"
							}
						}
					},
					"text": {
						"default": "var(--wp--preset|color|white)",
						"muted": "var(--wp--preset|color|primary-200)",
						"primary": "var(--wp--preset|color|primary-100)",
						"button": {
							"primary": {
								"default": "var(--wp--preset|color|white)",
								"hover": "var(--wp--preset|color|white)"
							}
						},
						"link": {
							"primary": {
								"default": "var(--wp--preset|color|primary-100)",
								"hover": "currentColor"
							}
						}
					},
					"icon": {
						"default": "currentColor",
						"muted": "var(--wp--preset|color|primary-950)",
						"primary": "var(--wp--preset|color|white)"
					},
					"border": {
						"default": "var(--wp--preset|color|primary-900)"
					}
				}
			},
			"global": {
				"border": {
					"style": "solid",
					"width": "1px",
					"radius": "0"
				}
			}
		},
		"color": {
