		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Caveat, 'Segoe Print', 'Bradley Hand', Chilanka, TSCu_Comic, casual, cursive",
					"slug": "primary",
					"name": "Primary (Caveat)",
					"fontFace": [
						{
							"fontFamily": "Caveat",
							"fontWeight": "400 700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./public/fonts/caveat/caveat.woff2" ]
						}
					]
				},
				{
					"fontFamily": "'Raleway', ui-sans-serif, sans-serif",
					"slug": "secondary",
					"name": "Secondary (Raleway)",
					"fontFace": [
						{
							"fontFamily": "Raleway",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./public/fonts/raleway/raleway.woff2"
							]
						},
						{
							"fontFamily": "Raleway",
							"fontWeight": "100 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [
								"file:./public/fonts/raleway/raleway-italic.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "ui-monospace, 'Fira Code', 'Cascadia Code', 'Source Code Pro', Menlo, Consolas, 'DejaVu Sans Mono', monospace",
					"slug": "mono",
					"name": "Monospace"
				}
			]
		}
	},
	"styles": {
		"border": {
			"top": {
				"color": "var(--wp--preset|color|primary-500)",
				"style": "solid",
				"width": "6px"
			},
			"bottom": {
				"color": "var(--wp--preset|color|primary-500)",
				"style": "solid",
				"width": "6px"
			}
		},
		"elements": {
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset|font-family|primary)",
					"fontSize": "var(--wp--preset|font-size|md)",
					"lineHeight": "var(--wp--custom|line-height|md)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "700"
				}
			}
		},
		"blocks": {
			"core/comments-title": {
				"typography": {
					"fontSize": "var(--wp--preset|font-size|xl)",
					"lineHeight": "var(--wp--custom|line-height|xl)"
				}
			},
			"core/image": {
				"typography": {
					"fontFamily": "var(--wp--preset|font-family|primary)",
					"fontSize": "var(--wp--preset|font-size|md)",
					"lineHeight": "var(--wp--custom|line-height|md)"
				}
			},
			"core/post-comments-form": {
				"elements": {
					"h3": {
						"typography": {
							"fontSize": "var(--wp--preset|font-size|lg)",
							"lineHeight": "var(--wp--custom|line-height|lg)"
						}
					}
				}
			},
			"core/pullquote": {
				"typography": {
					"fontFamily": "var(--wp--preset|font-family|primary)",
					"fontStyle": "italic"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset|font-family|secondary)",
							"fontSize": "clamp(var(--wp--preset--font-size--xs), 0.8125em, var(--wp--preset--font-size--md))",
							"fontStyle": "normal",
							"fontWeight": "300"
						}
					}
				},
				"css": "& blockquote {\n\tfont-size: 1.0625em;\n}"
			},
			"core/query-pagination": {
				"typography": {
					"fontFamily": "var(--wp--preset|font-family|primary)",
					"fontSize": "var(--wp--preset|font-size|lg)",
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom|line-height|lg)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/quote": {
				"elements": {
					"cite": {
						"typography": {
							"fontFamily": "var(--wp--preset|font-family|primary)",
							"fontSize": "1em",
							"fontStyle": "italic"
						}
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset|font-size|xl)",
					"fontWeight": "700",
					"letterSpacing": "-0.03em",
					"textTransform": "none"
				}
			},
			"core/verse": {
				"typography": {
					"fontFamily": "var(--wp--preset|font-family|primary)",
					"fontSize": "var(--wp--preset|font-size|lg)",
					"fontStyle": "italic",
					"lineHeight": "var(--wp--custom|line-height|lg)"
				}
			}
		},
		"variations": {
			"archive-header": {
				"border": {
					"bottom": {
						"color": "var(--wp--custom|border|primary|color)",
						"style": "var(--wp--custom|border|primary|style)",
						"width": "var(--wp--custom|border|primary|width)"
					}
				}
			},
			"meta": {
				"color": {
					"text": "var(--wp--custom|color|text|muted)"
				},
				"typography": {
					"fontWeight": "500"
				}
			},
			"site-footer": {
				"border": {
					"top": {
						"color": "var(--wp--preset|color|neutral-200)",
						"style": "solid",
						"width": "1px"
					}
				},
				"color": {
					"background": "var(--wp--preset|color|neutral-100)"
				}
			},
			"site-header": {
				"border": {
					"bottom": {
						"color": "var(--wp--custom|border|primary|color)",
						"style": "var(--wp--custom|border|primary|style)",
						"width": "var(--wp--custom|border|primary|width)"
					}
				},
				"color": {
					"background": "var(--wp--preset|color|neutral-50)"
				},
				"blocks": {
					"x3p0/breadcrumbs": {
						"border": {
							"top": {
								"color": "var(--wp--custom|border|primary|color)",
								"style": "var(--wp--custom|border|primary|style)",
								"width": "var(--wp--custom|border|primary|width)"
							}
						},
						"color": {
							"background": "var(--wp--preset|color|neutral-100)",
							"gradient": "none",
							"text": "var(--wp--preset|color|neutral-500)"
						},
						"typography": {
							"fontStyle": "italic",
							"fontSize": "var(--wp--preset|font-size|xs)",
							"lineHeight": "var(--wp--custom|line-height|xs)"
						},
						"elements": {
							"link": {
								"typography": {
									"fontStyle": "normal",
									"fontWeight": "500"
								}
							}
						}
					}
				}
			}
		}
	}
}
